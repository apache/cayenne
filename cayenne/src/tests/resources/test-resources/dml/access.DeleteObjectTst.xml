<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-lazy-init="true">	
	<!-- ======================================= -->
	<!-- Named Queries -->
	<!-- ======================================= -->
	<bean id="Artist1" class="org.objectstyle.cayenne.query.SqlModifyQuery">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.art.Artist</value></constructor-arg>
		<constructor-arg><value>
		insert into ARTIST (ARTIST_ID, ARTIST_NAME) values (1, 'artist1')
		</value></constructor-arg>
	</bean>
	<bean id="Painting1" class="org.objectstyle.cayenne.query.SqlModifyQuery">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.art.Painting</value></constructor-arg>
		<constructor-arg><value>
		insert into PAINTING (PAINTING_ID, PAINTING_TITLE, ARTIST_ID) values (1, 'painting1', 1)
		</value></constructor-arg>
	</bean>
	
	<!-- ======================================= -->
	<!-- Data Sets -->
	<!-- ======================================= -->	
	<bean id="testDeleteHollow" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="Artist1"/>
				<ref bean="Painting1"/>
			</list>
		</constructor-arg>
	</bean>	
</beans>