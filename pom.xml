	<profiles>
		
		<!-- Profile that generates assemblies after the build -->
		<profile>
			<id>assembly</id>
			<modules>
				<module>assembly</module>
			</modules>
		</profile>
		
		<!-- Optional profile used to sign artifacts -->
		<profile>
			<id>gpg</id>
			<build>
	        	<plugins>
	        		<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-gpg-plugin</artifactId>
						<executions>
						          <execution>
						            <id>sign-artifacts</id>
						            <phase>verify</phase>
						            <goals>
						              <goal>sign</goal>
						            </goals>
						          </execution>
							</executions>
	      			</plugin>
				</plugins>
			</build>
		</profile>
		
		<!-- Test DB Profiles (default is embedded HSQLDB) -->
		<profile>
			<id>default-db</id>
			<activation>
				<activeByDefault>true</activeByDefault>
				<property>
					<name>cayenne.test.connection</name>
