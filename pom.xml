										<goals>
											<goal>jjtree-javacc</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
								</pluginExecution>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.apache.cayenne.plugins</groupId>
										<artifactId>cayenne-maven-plugin</artifactId>
										<versionRange>[1.0,)</versionRange>
										<goals>
											<goal>cgen</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
